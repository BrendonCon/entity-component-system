{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": [
    "airbnb-base"
  ],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "import/extensions": [0],
    "import/no-unresolved": [0],
    "no-underscore-dangle": [0],
    "no-plusplus": [0],
    "arrow-parens": [0],
    "class-methods-use-this": [0],
    "comma-dangle": [0],
    "no-param-reassign": [0],
    "no-bitwise": [0]
  }
}